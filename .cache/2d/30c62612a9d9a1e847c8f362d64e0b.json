}\n\n\nheader {\n    height: auto;\n    background-color: $white;\n    color: $black;\n\n\n    h1 {\n        line-height: 0;\n        padding-top: 35px;\n    }\n\n    .logo {\n        padding-top: 20px;\n    }\n}\n\n.language-select {\n    padding-top: 60px;\n    color: #fff;\n    font-size: 1rem;\n\n}\n\n.nes-select {\n    width: 90%;\n}\n\n\nnav {\n    width: 100%;\n    height: 47px;\n    background-color: $cyan;\n    margin-top: 5px;\n    border: 3px solid $black;\n\n    font-size: 1.5rem;\n\n    ul {\n        list-style-type: none;\n        margin: 0;\n        padding: 0;\n        overflow: hidden;\n    }\n\n    li {\n        float: left;\n    }\n\n    li::first-letter {\n        text-decoration: underline;\n        border-bottom: 3px solid #000;\n    }\n\n    li a {\n        color: $black;\n        display: block;\n        text-align: center;\n        padding: 16px 16px;\n        text-decoration: none;\n    }\n\n    li a:hover:not(.active) {\n        background-color: #111;\n        color: $white;\n    }\n\n    .active {\n        background-color: $gray;\n    }\n}\n\n// SECTION STYLES\n\n.panel {\n    display: block;\n    padding-bottom: 4rem;\n    padding-top: 4rem;\n\n    // divider headers with double dashes\n    h5 {\n        text-align: center;\n        padding: 25px 15px 15px 15px;\n        border-right: none;\n        border-left: none;\n        text-transform: uppercase;\n        font-size: 1.25rem;\n    }\n\n    &.black {\n        height: auto;\n        width: 100%;\n        background-color: $black;\n        margin-left: auto;\n        margin-right: auto;\n        text-align: center;\n\n        h5 {\n\n            border-top: 2px dashed #fff;\n            border-bottom: 2px dashed #fff;\n        }\n    }\n\n    &.white {\n        height: 400px;\n        color: $black;\n        background-color: $white;\n\n        // divider headers with double dashes\n        h5 {\n\n            border-top: 2px dashed #000;\n            border-bottom: 2px dashed #000;\n        }\n    }\n\n\n}\n\n\n\n// TABLES\ntable {\n    border-collapse: collapse;\n\n}\n\nth {\n    border: 1px solid black;\n    border-bottom: 1px solid black;\n\n}\n// color the first row\ntbody tr:first-child td {\n   background: $cyan;\n}\n\ntr {\n    border-left: 1px solid black;\n    border-right: 1px solid black;\n\n}\n\ntd {\n    border-bottom: 1px solid black;\n\n}\n\n// DIVIDERS\n.section-title {\n    padding-bottom: 20px;\n}\n\n.divider {\n    background-image: url(\"../images/checkers.png\");\n    background-repeat: repeat-x;\n    height: 24px;\n\n}\n\n\nfooter {\n    width: 100vw;\n    background-color: $purple;\n    height: 100px;\n    padding-top: 20px;\n    text-align: center;\n    font-size: .25em;\n\n    nav {\n        background-color: $purple;\n        margin-top: 0;\n        border: 0;\n        color: $white;\n\n        a {\n            color: $white;\n            display: block;\n            text-align: center;\n            padding: 5px 16px;\n            text-decoration: none;\n\n        }\n\n        a:hover {\n            background-color: #111;\n            color: $white;\n        }\n    }\n}\n\n// COMPONENTS\n/* The Modal (background) */\n.modal {\n  display: none; /* Hidden by default */\n  position: fixed; /* Stay in place */\n  z-index: 1; /* Sit on top */\n  left: 0;\n  top: 0;\n  width: 100%; /* Full width */\n  height: 100%; /* Full height */\n  overflow: auto; /* Enable scroll if needed */\n  background-color: rgb(0,0,0); /* Fallback color */\n  background-color: rgba(0,0,0,0.4); /* Black w/ opacity */\n}\n\n/* Modal Content/Box */\n.modal-content {\n  background-color: #fefefe;\n  margin: 15% auto; /* 15% from the top and centered */\n  padding: 20px;\n  border: 1px solid #888;\n  width: 80%; /* Could be more or less, depending on screen size */\n}\n\n/* The Close Button */\n.close {\n  color: #aaa;\n  float: right;\n  font-size: 28px;\n  font-weight: bold;\n}\n\n.close:hover,\n.close:focus {\n  color: black;\n  text-decoration: none;\n  cursor: pointer;\n}\n// MS DOS APP WINDOW\n.ms-dos-window {\n    border: 2px solid $black;\n    display: flex;\n    justify-content: center;\n}\n\n.ms-dos-collapse {\n    border: 2px solid black;\n    display: flex;\n    justify-content: center;\n\n}\n\n.ms-icn-collapse {\n    text-align: center;\n}\n\n.ms-dos-arrow {}\n\n.teaser-content {\n    text-align: left;\n\n    p {\n        line-height: 2;\n        font-size: 1em;\n    }\n}\n\n// TEST COLORS DELETE AFTER PROD\n.color1 {\n    background-color: red;\n}\n\n.color2 {\n    background-color: blue;\n\n}\n\n.color3 {\n    background-color: magenta;\n}"},"lineCount":null}},"error":null,"hash":"398d21fc814eff81b0f41e5fbbce6360","cacheData":{"env":{}}}