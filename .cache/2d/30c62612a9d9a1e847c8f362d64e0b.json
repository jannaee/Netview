ay: flex;\n}\n\n.flex-item-center {\n    text-align: center;\n    justify-content: center;\n}\n\n.Section {\n    display: block;\n    padding-bottom: 9rem;\n    padding-top: 9rem;\n}\n.center-column {\n    width: 60%;\n    margin-left: auto;\n    margin-right: auto;\n    text-align: center;\n}\n\n// GENERAL STYLES\n\nfigure {\n    padding: 0;\n    img {\n        object-fit: cover;\n    }\n}\nbody {\n    font: 100% $font-stack;\n    color: $white;\n    padding: auto 15%;\n    overflow-x: hidden;\n    min-width: 400px;\n}\n\n\nheader {\n    height: auto;\n    background-color: $white;\n    color: $black;\n\n\n    h1 {\n        line-height: 0;\n        padding-top: 35px;\n    }\n\n    .logo {\n        padding-top: 20px;\n    }\n}\n\n.language-select {\n    padding-top: 60px;\n    color: #fff;\n    font-size: 1rem;\n\n}\n\n.nes-select {\n    width: 90%;\n}\n\n\nnav {\n    width: 100vw;\n    height: 47px;\n    background-color: $cyan;\n    margin-top: 5px;\n    border-top: 3px solid $black;\n    border-left: 3px solid $black;\n    border-right: 3px solid $black;\n    font-size: 1.5rem;\n\n    ul {\n        list-style-type: none;\n        margin: 0;\n        padding: 0;\n        overflow: hidden;\n    }\n\n    li {\n        float: left;\n    }\n\n    li::first-letter {\n        text-decoration: underline;\n        border-bottom: 3px solid #000;\n    }\n\n    li a {\n        color: $black;\n        display: block;\n        text-align: center;\n        padding: 16px 16px;\n        text-decoration: none;\n    }\n\n    li a:hover:not(.active) {\n        background-color: #111;\n        color: $white;\n    }\n\n    .active {\n        background-color: #4CAF50;\n    }\n}\n\n// SECTION STYLES\n.hero-section {\n    height: auto;\n    width: 100%;\n    background-color: $black;\n    margin-left: auto;\n    margin-right: auto;\n}\n\n.white-section {\n    height: 400px;\n    color: $black;\n    background-color: $white;\n\n    h5 {\n        text-align: center;\n        background: white;\n        padding: 25px 15px 15px 15px;\n        border-top: 2px dashed #000;\n        border-bottom: 2px dashed #000;\n        border-right: none;\n        border-left: none;\n        text-transform: uppercase;\n        font-size: 1.25rem;\n    }\n\n    article {\n        margin-top: 50px;\n    }\n\n}\n\n\n// DIVIDERS\n.divider {\n    background-image: url(\"../images/checkers.png\");\n    background-repeat: repeat-x;\n    height: 24px;\n\n}\n\n\nfooter {\n    width: 100vw;\n}\n\n// COMPONENTS\n// MS DOS APP WINDOW\n.ms-dos-window {\n    border: 2px solid $black;\n    display: flex;\n    justify-content: center;\n}\n\n.ms-dos-collapse {\n    border: 2px solid black;\n    display: flex;\n    justify-content: center;\n\n}\n\n.ms-icn-collapse {\n    text-align: center;\n}\n\n.ms-dos-arrow {}\n\n.teaser-content {}\n\n// TEST COLORS DELETE AFTER PROD\n.color1 {\n    background-color: red;\n}\n\n.color2 {\n    background-color: blue;\n\n}\n\n.color3 {\n    background-color: magenta;\n}"},"lineCount":null}},"error":null,"hash":"4a34cc431dca4b3778f80fbd8cd6e144","cacheData":{"env":{}}}