   background-color: #111;\n        color: $white;\n    }\n\n    .active {\n        background-color: $gray;\n    }\n}\n\n// SECTION STYLES\n.quotesWindow {\n    padding: 10px 25px 25px 25px;\n    line-height: 1.85;\n}\n\n.panel {\n    display: block;\n    padding-bottom: 4rem;\n    padding-top: 4rem;\n\n    // divider headers with double dashes\n    h5 {\n        text-align: center;\n        padding: 25px 15px 15px 15px;\n        border-right: none;\n        border-left: none;\n        text-transform: uppercase;\n        font-size: 1.25rem;\n    }\n\n    &.black {\n        height: auto;\n        width: 100%;\n        min-height: 200px;\n        background-color: $black;\n        margin-left: auto;\n        margin-right: auto;\n        text-align: center;\n\n        h5 {\n\n            border-top: 2px dashed #fff;\n            border-bottom: 2px dashed #fff;\n        }\n    }\n\n    &.white {\n        height: 100%;\n        min-height: 200px;\n        color: $black;\n        background-color: $white;\n\n        // divider headers with double dashes\n        h5 {\n\n            border-top: 2px dashed #000;\n            border-bottom: 2px dashed #000;\n        }\n    }\n\n\n}\n\n\n\n// TABLES\ntable {\n    border-collapse: collapse;\n    border-bottom: 1px solid $black;\n\n}\n\nth {\n    border: 1px solid black;\n    border-bottom: 1px solid black;\n\n}\n\n// color the first row\ntbody tr:first-child td {\n    background: $cyan;\n}\n\ntr {\n    border-left: 1px solid black;\n    border-right: 1px solid black;\n\n}\n\ntd {\n    border-bottom: 1px solid black;\n\n}\n\nthead {\n    text-align: center;\n}\n\n// DIVIDERS\n.section-title {\n    padding-bottom: 20px;\n}\n\n.divider {\n    background-image: url(\"../images/checkers.png\");\n    background-repeat: repeat-x;\n    height: 24px;\n\n}\n\n\nfooter {\n    width: 100vw;\n    background-color: $purple;\n    height: 100px;\n    padding-top: 20px;\n    text-align: center;\n    font-size: .25em;\n\n    nav {\n        background-color: $purple;\n        margin-top: 0;\n        border: 0;\n        color: $white;\n\n        a {\n            color: $white;\n            display: block;\n            text-align: center;\n            padding: 5px 16px;\n            text-decoration: none;\n\n        }\n\n        a:hover {\n            background-color: #111;\n            color: $white;\n        }\n    }\n}\n\n// COMPONENTS\n\n// Image Gallery\n\n.gallery-description {\n    text-align: left;\n    line-height: 1.85;\n    padding: 20px;\n}\n\n\n/* The Modal (background) */\n.modal {\n    display: none;\n    /* Hidden by default */\n    position: fixed;\n    /* Stay in place */\n    z-index: 1;\n    /* Sit on top */\n    left: 0;\n    top: 0;\n    width: 100%;\n    /* Full width */\n    height: 100%;\n    /* Full height */\n    overflow: auto;\n    /* Enable scroll if needed */\n    background-color: rgb(0, 0, 0);\n    /* Fallback color */\n    background-color: rgba($cyan, .9);\n    /* Black w/ opacity */\n}\n\n/* Modal Content/Box */\n.modal-content {\n    background-color: #fefefe;\n    margin: 15% auto;\n    /* 15% from the top and centered */\n    padding: 20px;\n    border: 3px solid $black;\n    width: 70%;\n    /* Could be more or less, depending on screen size */\n}\n\n.window-cta-btn {\n    padding-bottom: 50px;\n    border: 0;\n}\n\n.teaser-content {\n    text-align: left;\n\n    p {\n        line-height: 2;\n        font-size: 1em;\n    }\n}\n\n\n//nes overrides\n\nh3 {\n    width: 160px;\n    height: 41px;\n    background-color: red;\n    position: relative;\n    top: -39px;\n    white-space: nowrap;\n    font-size: 15px;\n    padding: 10px;\n}\n\n.nes-container.with-title>.title {\n    padding: 2rem;\n    width: auto;\n    margin-top: -4rem;\n    background-color: $black;\n\n}\n\n.nes-container.is-dark {\n    margin: 0;\n    background-color: $black;\n}"},"lineCount":null}},"error":null,"hash":"9f32e1e197bf23e61e52a53f38ef8512","cacheData":{"env":{}}}