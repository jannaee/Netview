order: 3px solid $black;\n\n\n    h1 {\n        text-align: left;\n        color: white;\n        padding: 25px;\n        font-size: 1.5rem;\n        white-space: nowrap;\n        margin: 0;\n        line-height: 0;\n    }\n\n    .logo {\n        padding-top: 20px;\n    }\n}\n\n.language-select {\n    padding-top: 60px;\n    color: #fff;\n    font-size: 1rem;\n\n}\n\n.nes-select {\n    width: 90%;\n}\n\n\nnav {\n    width: 100%;\n    height: 47px;\n    background-color: $cyan;\n\n    font-size: 12px;\n\n    ul {\n        list-style-type: none;\n        margin: 0;\n        padding: 0;\n        overflow: hidden;\n    }\n\n    li {\n        float: left;\n    }\n\n    li::first-letter {\n        text-decoration: underline;\n        border-bottom: 3px solid #000;\n    }\n\n    li a {\n        color: $black;\n        display: block;\n        text-align: center;\n        padding: 18px 16px;\n        text-decoration: none;\n    }\n\n    li a:hover:not(.active) {\n        background-color: $red;\n        color: $white;\ntext-decoration: none;\n\n    }\n\n    .active {\n        background-color: $gray;\n    }\n}\n\n// SECTION STYLES\n.quotesWindow {\n    padding: 10px 25px 25px 25px;\n    line-height: 1.85;\n}\n\n.panel {\n    display: block;\n    padding-bottom: 4rem;\n    padding-top: 4rem;\n\n    // divider headers with double dashes\n    h5 {\n        text-align: center;\n        padding: 25px 15px 15px 15px;\n        border-right: none;\n        border-left: none;\n        text-transform: uppercase;\n        font-size: 1.25rem;\n    }\n\n    &.black {\n        height: auto;\n        width: 100%;\n        min-height: 200px;\n        background-color: $black;\n        margin-left: auto;\n        margin-right: auto;\n        text-align: center;\n\n        h5 {\n\n            border-top: 2px dashed #fff;\n            border-bottom: 2px dashed #fff;\n        }\n    }\n\n    &.white {\n        height: 100%;\n        min-height: 200px;\n        color: $black;\n        background-color: $white;\n\n        // divider headers with double dashes\n        h5 {\n\n            border-top: 2px dashed #000;\n            border-bottom: 2px dashed #000;\n        }\n    }\n\n\n}\n\n\n\n// TABLES\n.ms-dos-table {\n\n    table {\n        border-collapse: collapse;\n        border-bottom: 1px solid $black;\n\n    }\n\n    th {\n        border: 1px solid black;\n        border-bottom: 1px solid black;\n\n    }\n\n\n    // color the first row\n    tbody tr:first-child td {\n        background: $cyan;\n    }\n\n    tr {\n        border-left: 1px solid black;\n        border-right: 1px solid black;\n\n    }\n\n    td {\n        border-bottom: 1px solid black;\n        // padding: 18px;\n\n    }\n\n    thead {\n        text-align: center;\n    }\n}\n\n// DIVIDERS\n.section-title {\n    padding-bottom: 20px;\n}\n\n.divider {\n    background-image: url(\"../images/checkers.png\");\n    background-repeat: repeat-x;\n    height: 24px;\n\n}\n\n\nfooter {\n    width: 100vw;\n    background-color: $purple;\n    height: 100px;\n    padding-top: 20px;\n    text-align: center;\n    font-size: .25em;\n\n    nav {\n        background-color: $purple;\n        margin-top: 0;\n        border: 0;\n        color: $white;\n\n        a {\n            color: $white;\n            display: block;\n            text-align: center;\n            padding: 5px 16px;\n            text-decoration: none;\n\n        }\n\n        a:hover {\n            background-color: #111;\n            color: $white;\n        }\n    }\n}\n\n// COMPONENTS\n\n/** MS DOS ITEMS **/\n.collapse-icn {\n    padding-left: 15px;\n}\n\n.down-icn {\n    padding-right: 15px;\n}\n\n.ms-file-title {\n    background-color: #534CFf;\n    color: white;\n    line-height: 1.85\n}\n\n\n// Image Gallery\n\n.gallery-description {\n    text-align: left;\n    line-height: 1.85;\n    padding: 20px;\n}\n\n\n\n.window-cta-btn {\n    padding-bottom: 50px;\n    border: 0;\n}\n\n.teaser-content {\n    text-align: left;\n\n    p {\n        line-height: 2;\n        font-size: 1em;\n    }\n}\n\n// MS DOS DROP DOWNS NAV\n.dropbtn {\n    background-color: $cyan;\n    border: none;\n    color: $black;\n\n    &::first-letter {\n        text-decoration: underline;\n        border-bottom: 3px solid #000;\n    }\n}\n\n.dropdown {\n    position: relative;\n    display: inline-block;\n}\n\n.dropdown-content {\n    display: none;\n    position: absolute;\n    background-color: $cyan;\n    min-width: 255px;\n    z-index: 1;\n    list-style-type: none;\n}\n\n.dropdown-content ul li {\n    padding-top: 10px;\n    list-style-type: none;\n    line-height: 1.8;\n    border-bottom: 1px solid $black;\n}\n\n.dropdown-content a {\n    padding: 10px 0;\n    margin: 20px;\n    white-space: nowrap;\n\n\n}\n\n.dropdown-content a:hover {\n    background-color: $red;\n    text-decoration: none;\n    color: $white;\n}\n\n.dropdown:hover .dropdown-content {\n    display: block;\n}\n\n.dropdown:hover .dropbtn {\n    background-color: $red;\n}\n\n// NES OVERRIDES\n\nh3 {\n    width: 160px;\n    height: 41px;\n    background-color: black;\n    position: relative;\n    top: -39px;\n    white-space: nowrap;\n    font-size: 15px;\n    padding: 10px;\n    color: $white;\n}\n\n\n.nes-container.with-title>.title {\n    padding: 2rem;\n    width: auto;\n    margin-top: -4rem;\n    background-color: $black;\n\n}\n\n.nes-container.is-dark {\n    margin: 0;\n    background-color: $black;\n}\n\n\n.nes-select {\n    margin: 5px 0;\n}\n\nbutton {\n    background-color:transparent;\n    border: none;\n\n}\nfigure {\n    margin: 0;\n}\n\n\n// MEDIA QUERIES\n@media only screen and (max-width: 700px) {\n    nav li {\n        font-size: 10px;\n        padding: 0;\n\n    }\n}"},"lineCount":null}},"error":null,"hash":"a65f12cec7679580291743d07a8cbf57","cacheData":{"env":{}}}