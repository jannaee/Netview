n: none;\n    }\n\n    li a:hover:not(.active) {\n        background-color: #111;\n        color: $white;\n    }\n\n    .active {\n        background-color: #4CAF50;\n    }\n}\n\n// SECTION STYLES\n.white-section {\n    height: 400px;\n    color: $black;\n    background-color: $white;\n\n    h5 {\n        text-align: center;\n        background: white;\n        padding: 15px;\n        border-top: 2px dashed #000;\n        border-bottom: 2px dashed #000;\n        border-right: none;\n        border-left: none;\n        text-transform: uppercase;\n        font-size: 1.25rem;\n\n\n    }\n}\n\n// .inspiration {\n// \twidth\n// }\n// DIVIDERS\n.divider {\n    background-image: url(\"images/checkers.png\");\n    background-repeat: repeat-x;\n    height: 24px;\n\n}\n\n\nfooter {\n    width: 100vw;\n}\n\n\n\n\n// TEST COLORS DELETE AFTER PROD\n.color1 {\n    background-color: red;\n}\n\n.color2 {\n    background-color: blue;\n\n}\n\n.color3 {\n    background-color: magenta;\n}"},"lineCount":null}},"error":null,"hash":"1a1eefff453d7a39a79e9efaca7b7019","cacheData":{"env":{}}}